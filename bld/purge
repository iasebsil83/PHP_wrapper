#!/bin/bash



# -------- DECLARATIONS --------

#exit on error
set -e

#current execution directory
CXD=$(dirname $(realpath $0))
pCXD=$(dirname $CXD)

#env
source $pCXD/cfg/srv.env






# -------- EXECUTION --------

#remove every generated files
$CXD/clean
echo "Also removing generated executables..."
rm --recursive --force $pCXD/out
sudo rm /etc/apache2/$NAME.conf
echo "Full removal done."
